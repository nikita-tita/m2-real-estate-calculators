{
  "version": 2,
  "name": "m2-calculators",
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.svg", "use": "@vercel/static" },
    { "src": "*.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" }
  ],
  "alias": ["calculators.m2.ru"],
  "github": {
    "autoAlias": true
  },
  "public": true
}